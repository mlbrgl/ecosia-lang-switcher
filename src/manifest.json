{
  "name": "Ecosia language switcher",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://www.ecosia.org/search*"],
      "js": ["app.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": ["cookies", "https://www.ecosia.org/"]
}
