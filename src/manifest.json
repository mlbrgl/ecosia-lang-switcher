{
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://www.ecosia.org/search*"],
      "js": ["app.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": ["cookies", "https://www.ecosia.org/"],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
